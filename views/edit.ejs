<%- include('./partials/header.ejs') %>

<div class="boc last">
    <div class="container">
        <div class="row">
            <div class="col-sm-6 mt-5">
                <h2 class="alert alert-success test-center">Update Student</h2>
                <form method="post">
                    <div class="form-group">
                        <label for="name">Name</label>
                        <input type="text" class="form-control" id="name" name="name" placeholder="Enter name">
                        
                      </div>
                      <div class="form-group">
                        <label for="email">Email address</label>
                        <input type="email" class="form-control" id="email"  placeholder="Enter email">
                        
                      </div>
                    <div class="form-group">
                      <label for="fees">Fees</label>
                      <input type="number" class="form-control" id="fees"  placeholder="Enter email">
                      
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Password</label>
                      <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
                    </div>
                    
                    <button  type="submit" class="btn btn-secondary ">Update</button>
                    
                  </form>
                  <form action="/student" method="get"><button  type="submit" class="btn btn-primary g">Back</button></form>
                  
            </div>
            
        </div>
    </div>
</div>

